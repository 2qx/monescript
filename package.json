{
  "name": "lui",
  "version": "0.0.1",
  "description": "JavaScript UI for ledger-cli files",
  "keywords": [
    "ledger-cli",
    "personal finance",
    "ui"
  ],
  "license": "GPL-3.0",
  "contributors": [],
  "devDependencies": {
    "jasmine": "^2.8.0"
  },
  "scripts": {
    "build-grammar": "pegjs src/resources/ledger-cli.pegjs",
    "build-ui-bundle": " browserify src/ui/main.js -o src/ui/_js/bundle.js",
    "build-ui-bundle-min": "uglify -s src/ui/bundle.js -o src/ui/bundle-min.js",
    "pretest": "npm run build-grammar",
    "test": "jasmine"
  },
  "dependencies": {
    "big.js": "^4.0.2",
    "moo": "^0.4.3",
    "pegjs": "^0.10.0",
    "underscore": "^1.8.3"
  }
}
